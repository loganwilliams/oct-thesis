keywords = {Analysis of Variance,Angiography,Angiography: methods,Animals,Anoxia,Anoxia: physiopathology,Cochlea,Cochlea: blood supply,Imaging, Three-Dimensional,Imaging, Three-Dimensional: methods,Laser-Doppler Flowmetry,Laser-Doppler Flowmetry: methods,Male,Mice,Mice, Inbred C57BL,Microtechnology,Regional Blood Flow,Regional Blood Flow: physiology},
month = oct,
number = {10},
