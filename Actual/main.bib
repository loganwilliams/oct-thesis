pages = {271--7},
pmid = {19428537},
title = {{A digital heterodyne laser interferometer for studying cochlear mechanics.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19428537},
volume = {179},
year = {2009}
}
@article{Ren2011,
abstract = {The extraordinary sensitivity of the mammalian ear is commonly attributed to the cochlear amplifier, a cellular process thought to locally boost responses of the cochlear partition to soft sounds. However, cochlear power gain has not been measured directly. Here we use a scanning laser interferometer to determine the volume displacement and volume velocity of the cochlear partition by measuring its transverse vibration along and across the partition. We show the transverse displacement at the peak-response location can be >1,000 times greater than the displacement of the stapes, whereas the volume displacement of an area centred at this location is approximately tenfold greater than that of the stapes. Using the volume velocity and cochlear-fluid impedance, we discover that power at the peak-response area is >100-fold greater than that at the stapes. These results demonstrate experimentally that the cochlea amplifies soft sounds, offering insight into the mechanism responsible for the cochlear sensitivity.},
author = {Ren, Tianying and He, Wenxuan and Gillespie, Peter G},
doi = {10.1038/ncomms1226},
file = {:Users/loganw/Documents/Journal articles/Mendeley/Ren, He, Gillespie/Nature communications/Ren, He, Gillespie - 2011 - Measurement of cochlear power gain in the sensitive gerbil ear.pdf:pdf},
issn = {2041-1723},
journal = {Nature communications},
keywords = {Animals,Basilar Membrane,Basilar Membrane: chemistry,Basilar Membrane: physiology,Cochlea,Cochlea: chemistry,Cochlea: physiology,Ear,Ear: physiology,Gerbillinae,Humans,Interferometry,Models, Animal,Vibration},
month = jan,
pages = {216},
pmid = {21364555},
publisher = {Nature Publishing Group},
title = {{Measurement of cochlear power gain in the sensitive gerbil ear.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21364555},
volume = {2},
year = {2011}
}



