@article{Nuttall2012,
abstract = {Georg von B\'{e}k\'{e}sy designed the instruments needed for his research. He also created physical models of the cochlea allowing him to manipulate the parameters (such as volume elasticity) that could be involved in controlling traveling waves. This review is about the specific devices that he used to study the motion of the basilar membrane thus allowing the analysis that lead to his Nobel Prize Award. The review moves forward in time mentioning the subsequent use of von B\'{e}k\'{e}sy's methods and later technologies important for motion studies of the organ of Corti. Some of the seminal findings and the controversies of cochlear mechanics are mentioned in relation to the technical developments.},
